global OnMapLoad = function()
{
	if ( TestMapOn )
		{ Util.AutoTestMap(); }

	OnTrigger( "MISSING_STRING", Map.BUILD_1The_Axis_3Backyard_MG42_Construction_Built );
	OnTrigger( "MISSING_STRING", Map.BUILD_1the_Axis_3Watchtower_MG42_Construction_Built );
	OnTrigger( "Trolleybus Barrier #1 has been constructed.", Map.BUILD_2Henschel_Trolleybus_Barrier_1_Built );
	OnTrigger( "Trolleybus Barrier #2 has been constructed.", Map.BUILD_2Henschel_Trolleybus__Barrier_2_Built );
	OnTrigger( "MISSING_STRING", Map.BUILD_2the_Command_Post_Built );
	OnTrigger( "Allied team has secure the key and stolen the trolleyBus!", Map.BUILD_2the_Henschel_Trolleybus_Built );
	OnTrigger( "MISSING_STRING", Map.BUILD_4The_Allied_3Road_MG42_Construction_Built );
	OnTrigger( "MISSING_STRING", Map.BUILD_4the_Allied_3Watchtower_MG42_Construction_Built );
	OnTrigger( "MISSING_STRING", Map.PLANT_1The_Axis_3Backyard_MG42_Construction_Destroyed );
	OnTrigger( "MISSING_STRING", Map.PLANT_1the_Axis_3Watchtower_MG42_Construction_Destroyed );
	OnTrigger( "Trolleybus Barrier #1 has been destroyed.", Map.PLANT_2Henschel_Trolleybus_Barrier_1_Destroyed );
	OnTrigger( "Trolleybus Barrier #2 has been destroyed.", Map.PLANT_2Henschel_Trolleybus__Barrier_2_Destroyed );
	OnTrigger( "MISSING_STRING", Map.PLANT_2the_Command_Post_Destroyed );
	OnTrigger( "MISSING_STRING", Map.PLANT_4The_Allied_3Road_MG42_Construction_Destroyed );
	OnTrigger( "MISSING_STRING", Map.PLANT_4the_Allied_3Watchtower_MG42_Construction_Destroyed );
	OnTrigger( "MISSING_STRING", Map.PLANT_8The_1Axis_8Back_Door_Destroyed );
	OnTrigger( "MISSING_STRING", Map.PLANT_8The_1Axis_8Back_Door_1_Destroyed );
	OnTrigger( "MISSING_STRING", Map.PLANT_8The_1Axis_8MainBack_Door_Destroyed );
	OnTrigger( "MISSING_STRING", Map.PLANT_8The_1Axis_8MainBack_Door2_Destroyed );
	OnTrigger( "MISSING_STRING", Map.bus_key_Taken );
/*
	SetAvailableMapGoals( TEAM.AXIS, false, ".*" );
	SetAvailableMapGoals( TEAM.ALLIES, false, ".*" );
*/
	Util.MapDebugPrint( "OnMapLoad" );
};
