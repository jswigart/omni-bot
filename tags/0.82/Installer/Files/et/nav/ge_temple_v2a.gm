	Maproutes.DEFEND_axis_relic4 = Maproutes.DEFEND_axis_relic1;
	Maproutes.DEFEND_axis_relic5 = Maproutes.DEFEND_axis_relic1;
	Maproutes.DEFEND_axis_relic6 = Maproutes.DEFEND_axis_relic1;
