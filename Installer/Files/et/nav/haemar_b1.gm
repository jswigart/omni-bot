				if ( !Map.TriggerOneVoiceChat && MAP_TALK ) {
					Util.OneBotVSayTeam( TEAM.AXIS, VOICE.DEFEND_OBJECTIVE );
					Map.TriggerOneVoiceChat = true;
				}
			}
		},
		OnExit = function(ent)
		{
		},
	},

