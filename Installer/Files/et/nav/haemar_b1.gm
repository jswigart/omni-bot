		SetAvailableMapGoals( TEAM.ALLIES, false, "ATTACK_gate.*" );
		Util.SetGoalPosition( 2085.45, 1077.5, 112.125, "PLANT_City_Gate" ) ;
		Util.MapDebugPrint("Allied_Assault_Ramp_Built");
	},

