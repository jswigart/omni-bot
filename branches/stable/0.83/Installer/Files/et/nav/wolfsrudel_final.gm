			SetAvailableMapGoals( TEAM.ALLIES, false, "ATTACK_mast.*" );
		}
		Util.EnableGoal( "FLAG.*" );
		SetAvailableMapGoals( TEAM.ALLIES, true, "ROUTE_sidewall_right" );
