	SetMapGoalProperties( "ATTACK_.*", {mincamptime = 15, maxcamptime = 20} );
	SetMapGoalProperties( "DEFEND_.*", {mincamptime = 15, maxcamptime = 30} );
	SetMapGoalProperties( "PANZER_.*", {mincamptime = 45, maxcamptime = 90} );

