global OnBotJoin = function( bot )
{
	bot.MaxViewDistance = 2500;

	Util.IgnoreTargetGoal( bot, "MOVER_church_rubble" );

	if ( Map.RelicSecured )
		{ Util.IgnoreTargetGoal( bot, "MOVER_tank" ); }

	if ( Map.tank_past_gun )
		{ Util.IgnoreTargetGoal( bot, "MOVER_at_gun" ); }
};

