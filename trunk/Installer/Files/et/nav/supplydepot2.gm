	Forward_Bunker_Gate_Dyno = 0,
	Forward_spawn_rear_exit_Dyno = 0,
	West_Depot_Wall_Dyno = 0,

	// Objective status
	FlagStatus = 0, // nobody owns it. 1 = axis, 2 = allied
