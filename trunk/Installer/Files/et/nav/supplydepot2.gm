	Crane_Activated = function( trigger )
	{
		if ( TestMap ) { return; }

		// mark the crane as activated
		Map.CraneActivated = true;

		// and shift focus to truck
		Util.DisableGroup( "rcranecontrols", TEAM.AXIS );
		SetAvailableMapGoals( TEAM.AXIS, false, "PLANT_Crane_Controls" );
		SetAvailableMapGoals( TEAM.AXIS, false, "BUILD_Depot_Fence" );
		Util.DisableGroup( "binnerdepot", TEAM.ALLIES );
		SetAvailableMapGoals( TEAM.ALLIES, false, "SWITCH_crane" );
		SetAvailableMapGoals( TEAM.ALLIES, false, "PLANT_Depot_Fence" );
		SetAvailableMapGoals( TEAM.ALLIES, false, "DEFUSE_Crane_Controls.*" );
		SetGoalPriority( "DEFUSE_Crane_Controls.*", 0.0, TEAM.ALLIES, CLASS.ENGINEER, true );

		// re-use some of the axis inner depot defense
		SetAvailableMapGoals( TEAM.AXIS, true, "DEFEND_rInnerDepot[123456]" );
		SetAvailableMapGoals( TEAM.AXIS, true, "DEFEND_rCrateLoad.*" );
		SetAvailableMapGoals( TEAM.AXIS, true, "CALLARTILLERY_rInnerDepot.*" );
		SetAvailableMapGoals( TEAM.AXIS, true, "ARTILLERY_S_rInnerDepot.*" );
		SetAvailableMapGoals( TEAM.AXIS, true, "PLANTMINE_rEastWall.*" );
		SetAvailableMapGoals( TEAM.ALLIES, true, "ATTACK_bTruckAssist.*" );
		SetAvailableMapGoals( TEAM.ALLIES, true, "ATTACK_bEscape.*" );

		Util.MapDebugPrint( "Crane_Activated" );
	},

