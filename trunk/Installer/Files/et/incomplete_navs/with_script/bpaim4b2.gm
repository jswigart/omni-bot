{
	if ( TestMapOn )
	{
		Util.AutoTestMap();
	}
	
	SetMapGoalProperties( "ATTACK_.*", {mincamptime=3, maxcamptime=6});
	SetMapGoalProperties( "DEFEND_.*", {mincamptime=20, maxcamptime=30});
};

