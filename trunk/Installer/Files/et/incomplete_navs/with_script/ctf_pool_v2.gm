{
	//bot.TargetBreakableDist = 100.0;
	bot.MaxViewDistance = 700;
	
	Util.IgnoreTargetGoal( bot, "MOVER_bullseye" );
	Util.IgnoreTargetGoal( bot, "MOVER_dogs" );
	Util.IgnoreTargetGoal( bot, "MOVER_coi" );
	Util.IgnoreTargetGoal( bot, "MOVER_telly" );
};
