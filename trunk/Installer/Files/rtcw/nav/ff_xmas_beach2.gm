	alive_watch = function()
	{
		while ( 1 )
		{
			if ( !Map.DocStatus && !Map.WarRoomShift && RTCWUtil.IsTeamDead( TEAM.ALLIES ) ) {
				// guess there should be a spawn time check too ...
				SetAvailableMapGoals( TEAM.AXIS, true, "DEFUSE.*" );
			}

			sleep( 1 );
		}
	},
