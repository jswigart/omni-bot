		SetAvailableMapGoals( TEAM.AXIS, true, "VENOM_rUpper.*" );
		SetAvailableMapGoals( TEAM.AXIS, true, "FLAME_rUpper.*" );

		//set these back to default ( changed in the stairs triggers )
