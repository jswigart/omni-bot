		//if(Map.FlagStatus == 2)
		//{
		Map.AxisASTable = {"rTrenchAS_1","rTrenchAS_2",};
		//}

		if ( Map.CompSettings ) {
			RTCWUtil.SetTeamSuicide( TEAM.AXIS, 1, 1 );
